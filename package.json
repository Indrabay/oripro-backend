{
  "name": "oripro-backend",
  "version": "1.0.0",
  "description": "Express backend with a login endpoint",
  "main": "src/app.js",
  "type": "commonjs",
  "scripts": {
    "start": "DOTENV_CONFIG_PATH=./.env node -r dotenv/config src/app.js",
    "dev": "DOTENV_CONFIG_PATH=./.env nodemon --watch src -r dotenv/config src/app.js",
    "migrate": "DOTENV_CONFIG_PATH=./.env node -r dotenv/config scripts/runMigrations.js",
    "seed": "DOTENV_CONFIG_PATH=./.env node -r dotenv/config scripts/seedDemoUser.js"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0"
  }
}

