{
  "name": "oripro-backend",
  "version": "1.0.0",
  "description": "Express backend with a login endpoint",
  "main": "src/app.js",
  "type": "commonjs",
  "scripts": {
    "start": "node --env_file=./.env -r dotenv/config src/app.js",
    "dev": "nodemon --env_file=./.env --watch src -r dotenv/config src/app.js",
    "migrate": "node --env_file=./.env -r dotenv/config scripts/runMigrations.js",
    "seed": "node --env_file=./.env -r dotenv/config scripts/seedDemoUser.js",
    "seed:roles": "node --env_file=./.env -r dotenv/config scripts/seedRoles.js",
    "seed:admin": "node --env_file=./.env -r dotenv/config scripts/seedAdminUser.js",
    "seed:menus": "node --env_file=./.env -r dotenv/config scripts/seedMenus.js",
    "seed:all": "node --env_file=./.env -r dotenv/config scripts/seedAll.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "mysql2": "^3.14.5",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.14",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.3"
  }
}
